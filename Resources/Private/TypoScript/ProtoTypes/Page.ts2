prototype(TYPO3.Neos:Page) {

    head {

        jqueryJs = TYPO3.TypoScript:Tag {
            @position = 'before javascripts'
            tagName = 'script'
            attributes {
                type = 'text/javascript'
                src = TYPO3.TypoScript:ResourceUri {
                    path = 'resource://ObisConcept.NeosJQuery/Public/JavaScript/jquery.min.js'
                }
            }
        }

    }

}
prototype(Neos.Neos:Page) {

    head {

        jqueryJs = Neos.Fusion:Tag {
            @position = 'before javascripts'
            tagName = 'script'
            attributes {
                type = 'text/javascript'
                src = Neos.Fusion:ResourceUri {
                    path = 'resource://ObisConcept.NeosJQuery/Public/JavaScript/jquery.min.js'
                }
            }
        }

    }

}